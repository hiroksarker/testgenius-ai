{
  "name": "testgenius-ai",
  "version": "1.0.0",
  "description": "AI-Powered E2E Testing Framework with WebdriverIO - Record, generate, and execute comprehensive test scenarios with intelligent automation",
  "main": "dist/bin/testgenius.js",
  "bin": {
    "testgenius": "dist/bin/testgenius.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "CONTRIBUTING.md",
    "CODE_OF_CONDUCT.md",
    "SECURITY.md",
    "WIKI_HOME.md",
    "QUICKSTART.md",
    "env.example",
    "testgenius.config.js"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "start": "node dist/bin/testgenius.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist",
    "setup:env": "bash setup-env.sh",
    "allure:generate": "allure generate allure-results --clean -o allure-report",
    "allure:open": "allure open allure-report",
    "allure:serve": "allure serve allure-results",
    "allure:clean": "rm -rf allure-results allure-report",
    "prepublishOnly": "npm run build && npm test",
    "prepack": "npm run build"
  },
  "keywords": [
    "e2e-testing",
    "webdriverio",
    "ai-testing",
    "automation",
    "typescript",
    "allure",
    "bdd",
    "test-automation",
    "selenium",
    "browser-testing",
    "test-recorder",
    "smart-testing",
    "ai-powered",
    "end-to-end",
    "testing-framework",
    "chrome-automation",
    "firefox-automation",
    "safari-automation",
    "edge-automation",
    "test-reporting"
  ],
  "author": "Hirok Sarker <hirok.sarker@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hiroksarker/testgenius-ai.git"
  },
  "bugs": {
    "url": "https://github.com/hiroksarker/testgenius-ai/issues"
  },
  "homepage": "https://github.com/hiroksarker/testgenius-ai#readme",
  "support": {
    "email": "support@testgenius.ai",
    "issues": "https://github.com/hiroksarker/testgenius-ai/issues",
    "discussions": "https://github.com/hiroksarker/testgenius-ai/discussions",
    "wiki": "https://github.com/hiroksarker/testgenius-ai/wiki"
  },
  "dependencies": {
    "webdriverio": "^8.32.5",
    "allure-commandline": "^2.24.1",
    "dotenv": "^16.3.1",
    "commander": "^11.1.0",
    "chalk": "^4.1.2",
    "inquirer": "^8.2.6",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^8.2.10",
    "@types/glob": "^8.1.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "publishConfig": {
    "access": "public"
  },
  "man": "./docs/man/testgenius.1"
}
